{
  "name": "react-editext",
  "version": "4.2.1",
  "description": "Editable Text Component for React Applications",
  "keywords": [
    "react",
    "editable",
    "editable text",
    "editable content",
    "contenteditable",
    "content editable",
    "inline edit"
  ],
  "author": "alioguzhan",
  "license": "MIT",
  "repository": "alioguzhan/react-editext",
  "main": "dist/index.min.js",
  "module": "dist/index.es.js",
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "rollup -c rollup.config.js -w",
    "build": "rollup -c rollup.config.js",
    "coverage": "codecov -e TRAVIS_NODE_VERSION -f coverage/*.json",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "test:ci": "cross-env CI=1 jest",
    "predeploy": "cd example && yarn install && yarn run build && cp _redirects build/",
    "deploy": "gh-pages -d example/build",
    "release": "standard-version",
    "prepare": "husky install"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "./node_modules/.bin/prettier --write",
      "./node_modules/.bin/eslint"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.1",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.2",
    "@types/react": "^17.0.25",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "codecov": "^3.8.3",
    "colors": "^1.4.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.2.4",
    "lint-staged": "^11.1.2",
    "postcss": "^8.3.8",
    "prettier": "^2.4.1",
    "prompt": "^1.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "replace-in-file": "^6.2.0",
    "rollup": "^2.57.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.1",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "shelljs": "^0.8.4",
    "standard-version": "^9.3.1",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3",
    "typescript-plugin-css-modules": "^3.4.0"
  },
  "dependencies": {}
}
